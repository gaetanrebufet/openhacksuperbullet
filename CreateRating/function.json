{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "doc",
      "databaseName": "openhack",
      "collectionName": "ratings",
      "createIfNotExists": "true",
      "connectionStringSetting": "https://cosmorating.documents.azure.com:443/"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
